package JavaLar;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
	
		Python python = new Python(4, 0);
		int instantes = scanner.nextInt();
		
		System.out.println("Passaram-se " + python.horasPassadas(instantes) + " em Python!");
		

		
		
		scanner.close();
	}

}
